<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <!--70c656-->
            <!--dcd40b-->
            <solid
                    android:color="#197e3e" />
            <stroke
                    android:width="1dp"
                    android:color="#53933f" />
            <corners
                    android:radius="3dp" />
            <padding
                    android:left="20dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp" />
        </shape>
    </item>
    <item android:state_enabled="false" >
        <shape>
            <solid
                    android:color="#111111"/>
            <stroke
                    android:width="1dp"
                    android:color="#111111"/>

            <corners
                    android:radius="3dp" />
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp" />
        </shape>
    </item>
    <!-- android:startColor="#70c656"
                        android:endColor="#53933f"
                        android:startColor="#b1bb1c"
                    android:endColor="#6ea12c"

                        -->
    <item>
        <shape>
            <gradient
                    android:startColor="#89ab26"
                    android:endColor="#4f9333"
                    android:angle="270" />
            <stroke
                    android:width="1dp"
                    android:color="#53933f" />
            <corners
                    android:radius="4dp" />
            <padding
                    android:left="10dp"
                    android:top="10dp"
                    android:right="10dp"
                    android:bottom="10dp" />
        </shape>
    </item>
</selector>